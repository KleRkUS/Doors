
<div class="container catalog-container">

	<h1><%= title%></h1>

	<div class="row catalog-row">

		<% for (item of data) {%>
		
			<div class="col-xl-3 col-lg-3 col-md-3 col-sm-4 col-xs-6">
				<div class="catalog-block">
					<h2 class="catalog-header"><%= item.name%></h2>
					<img src="<%= item.link%>" alt="<%= item.name%>" class="catalog-img">
					<p class="catalog-description"><%= item.description%></p>
					<span class="catalog-price"><%= item.price%>&#8381;</span>
				</div>
			</div>

		<%}%>

	</div>
</div>